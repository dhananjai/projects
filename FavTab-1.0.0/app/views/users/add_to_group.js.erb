
jQuery("#add_to_group_popup").fadeOut().remove();
<% @created_g_u.each do |g_u| %>
  var output_html = ("<%= escape_javascript(render :partial => "group_user", :locals=>{:g_u=>g_u}).html_safe %>");
  jQuery("#group_<%= g_u.group.id %> .group_users_container").append(output_html);
<% end %>
display_flash_message("<%= @notice %>");
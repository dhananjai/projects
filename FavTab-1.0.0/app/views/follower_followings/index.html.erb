<h1>Listing follower_followings</h1>

<table>
  <tr>
    <th>Follower</th>
    <th>Following</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @follower_followings.each do |follower_following| %>
  <tr>
    <td><%= follower_following.follower_id %></td>
    <td><%= follower_following.following_id %></td>
    <td><%= link_to 'Show', follower_following %></td>
    <td><%= link_to 'Edit', edit_follower_following_path(follower_following) %></td>
    <td><%= link_to 'Destroy', follower_following, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

